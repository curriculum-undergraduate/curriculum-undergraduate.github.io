orm login section -->\r\n              <form action=\"\" class=\"mt-5\">\r\n                <div class=\"mb-3\">\r\n                  <label for=\"InputName\" class=\"form-label\">Name</label>\r\n                  <input\r\n                    type=\"text\"\r\n                    id=\"InputName\"\r\n                    class=\"form-control text-indent shadow-sm bg-grey-light border-0 rounded-pill fw-lighter fs-7 p-3\"\r\n                    placeholder=\"Your Name\"\r\n                  />\r\n                </div>\r\n                <div class=\"mb-3\">\r\n                  <label for=\"InputEmail\" class=\"form-label\">Email</label>\r\n                  <input\r\n                    type=\"email\"\r\n                    id=\"InputEmail\"\r\n                    class=\"form-control text-indent shadow-sm bg-grey-light border-0 rounded-pill fw-lighter fs-7 p-3\"\r\n                    placeholder=\"name@example.com\"\r\n                  />\r\n                </div>\r\n                <div class=\"mb-4\">\r\n                  <label for=\"InputPassword\" class=\"form-label\">Password</label>\r\n                  <div class=\"d-flex position-relative\">\r\n                    <input v-if=\"showPassword1\" v-model=\"password1\"\r\n                      type=\"text\"\r\n                      id=\"InputPassword\"\r\n                      name=\"password\"\r\n                      placeholder=\"your password\"\r\n                      class=\"form-control text-indent auth__password shadow-sm bg-grey-light border-0 rounded-pill fw-lighter fs-7 p-3\"\r\n                    />\r\n                    <input v-else v-model=\"password1\"\r\n                      type=\"password\"\r\n                      id=\"InputPassword\"\r\n                      name=\"password\"\r\n                      placeholder=\"your password\"\r\n                      class=\"form-control text-indent auth__password shadow-sm bg-grey-light border-0 rounded-pill fw-lighter fs-7 p-3\"\r\n                    />\r\n                    <span @click=\"togglePassword1\"\r\n                      class=\"password__icon text-dark fs-4 fw-bold\"\r\n                      :class=\"{\r\n                        'bi bi-eye': showPassword1,\r\n                        'bi bi-eye-slash': !showPassword1,\r\n                      }\"\r\n                    ></span>\r\n                  </div>\r\n                </div>\r\n                <div class=\"mb-4\">\r\n                  <label for=\"InputConfirmPassword\" class=\"form-label\"\r\n                    >Confirm Password</label\r\n                  >\r\n                  <div class=\"d-flex position-relative\">\r\n                    <input v-if=\"showPassword2\" v-model=\"password2\"\r\n                      type=\"text\"\r\n                      id=\"InputConfirmPassword\"\r\n                      name=\"confirm_password\"\r\n                      placeholder=\"confirm password\"\r\n                      class=\"form-control text-indent auth__password shadow-sm bg-grey-light border-0 rounded-pill fw-lighter fs-7 p-3\"\r\n                    />\r\n                    <input v-else v-model=\"password2\"\r\n                      type=\"password\"\r\n                      id=\"InputConfirmPassword\"\r\n                      name=\"confirm_password\"\r\n                      placeholder=\"confirm password\"\r\n                      class=\"form-control text-indent auth__password shadow-sm bg-grey-light border-0 rounded-pill fw-lighter fs-7 p-3\"\r\n                    />\r\n                    <span @click=\"togglePassword2\"\r\n                      class=\"password__icon text-dark fs-4 fw-bold\"\r\n                      :class=\"{\r\n                        'bi bi-eye': showPassword2,\r\n                        'bi bi-eye-slash': !showPassword2,\r\n                      }\"\r\n                    ></span>\r\n                  </div>\r\n                </div>\r\n                <div class=\"pt-3\" style=\"margin-bottom: -10px\">\r\n                  <div class=\"row mb-3\">\r\n                    <div class=\"form-check\">\r\n                      <input\r\n                        type=\"checkbox\"\r\n                        class=\"form-check-input\"\r\n                        id=\"dropdownCheck\"\r\n                      />\r\n                      <label class=\"form-check-label\" for=\"dropdownCheck\">\r\n                        By signing in, I agree to the CU Privacy Statement and\r\n                        Terms of Service.\r\n                      </label>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col text-center\">\r\n                  <button\r\n                    type=\"submit\"\r\n                    class=\"btn btn-outline-light btn-lg rounded-pill mt-4 w-100\"\r\n                  >\r\n                    Register\r\n                  </button>\r\n                </div>\r\n              </form>\r\n              <div class=\"separator mt-5 text-white\">\r\n                Or Sign up with social platforms\r\n              </div>\r\n              <div class=\"row text-center\" style=\"color: #ffffff\">\r\n                <div class=\"col mt-3\">\r\n                  <a\r\n                    href=\"\"\r\n                    class=\"btn btn-outline-light border-2 rounded-thumbnail\"\r\n                    ><i class=\"bi bi-facebook fs-5\"></i\r\n                  ></a>\r\n                </div>\r\n                <div class=\"col mt-3\">\r\n                  <a\r\n                    href=\"\"\r\n                    class=\"btn btn-outline-light border-2 rounded-thumbnail\"\r\n                    ><i class=\"bi bi-linkedin fs-5\"></i\r\n                  ></a>\r\n                </div>\r\n                <div class=\"col mt-3\">\r\n                  <a\r\n                    href=\"\"\r\n                    class=\"btn btn-outline-light border-2 rounded-thumbnail\"\r\n                    ><i class=\"bi bi-twitter fs-5\"></i\r\n                  ></a>\r\n                </div>\r\n                <div class=\"col my-3\">\r\n                  <a\r\n                    href=\"\"\r\n                    class=\"btn btn-outline-light border-2 rounded-thumbnail\"\r\n                    ><i class=\"bi bi-google fs-5\"></i\r\n                  ></a>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"d-none d-lg-block col-lg-6 col-xl-6\">\r\n      <div class=\"row vh-100\" style=\"padding-top: 35%; margin-left: 113%\">\r\n        <div class=\"col align-self-center text-center\">\r\n          <img src=\"../assets/img/welcome.png\" width=\"350\" class=\"bounce\" alt=\"\" />\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nexport default {\r\n    name: 'App',\r\n    data() {\r\n        return {\r\n            showPassword1: false,\r\n            password1: null,\r\n            showPassword2: false,\r\n            password2: null\r\n        }\r\n    },\r\n    methods: {\r\n        togglePassword1() {\r\n            this.showPassword1 = !this.showPassword1;\r\n        },\r\n        togglePassword2() {\r\n            this.showPassword2 = !this.showPassword2;\r\n        }\r\n    }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module"}